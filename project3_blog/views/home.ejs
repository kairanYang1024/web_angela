<%-include("partials/header")-%>

<h1>Home</h1>
<p><%=homeStartingContent%></p>

<h1>Posts:</h1>
<% posts.forEach((post)=>{ %>
    <div class="mb-3">
        <h2><%=post.title%></h2>
        <p><%= post.content.substring(0, 100) + "..." %> 
            <a href="/posts/<%=post._id%>">Read More</a>
        </p>
    </div>
<% }); %>

<%-include("partials/footer")-%>